{
  "manifest_version": 2,
 
  "name": "sID",
  "description": "Chromium Extension for sID",
  "version": "1.0",
  "permissions": [ "webNavigation", "activeTab" , "tabs", "http://*/*", "https://*/*", "unlimitedStorage", "https://www.facebook.com/*", "https://web.facebook.com/*" ],
  
  "optional_permissions": [ "webRequest" ],
     
  "browser_action": {
   "default_icon": "resources/icons/ext.png",
   "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["js/background.js"],
    "persistent": true
  },
  "web_accessible_resources": [
    "resources/icons/claimT.png" , "resources/icons/claimR.png" , "resources/icons/claimC.png",
	"resources/icons/profT.png" , "resources/icons/profR.png" , "resources/icons/profC.png"  , 
	"resources/icons/prof_li_T.png" , "resources/icons/prof_li_R.png" , "resources/icons/prof_li_C.png"  ,
	"resources/icons/ringT.png", "resources/icons/ringR.png", "resources/icons/ringC.png",
	"resources/icons/popupBase.png",
	"html/sidAnalytics.html", "html/ratePopup.html"
  ],
  "content_scripts":[
	  {
		"matches": ["http://*/*","https://*/*"],
		"css": ["css/dropdown.css","css/popUpStyles.css"],
		"html":["html/ratePopup.html"]
	  }
  ]
}